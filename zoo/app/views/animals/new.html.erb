<h1>Add Animal</h1>
<p>Find me in app/views/animals/new.html.erb</p>

<%= form_for @animal do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :species %>
  <%= f.text_field :species %>

  <%= f.label :age %>
  <%= f.text_field :age %>

  <%= f.label :fav_food %>
  <%= f.text_field :fav_food %>

  <%= f.submit %>
<% end %>
